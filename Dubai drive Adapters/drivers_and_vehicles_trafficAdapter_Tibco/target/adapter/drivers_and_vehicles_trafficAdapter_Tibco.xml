<mfp:adapter name="drivers_and_vehicles_trafficAdapter_Tibco" xmlns:http="http://www.ibm.com/mfp/integration/http" xmlns:mfp="http://www.ibm.com/mfp/integration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <displayName>drivers_and_vehicles_trafficAdapter_Tibco</displayName>
    <description>drivers_and_vehicles_trafficAdapter_Tibco</description>
