<mfp:adapter name="nolUploadService" xmlns:http="http://www.ibm.com/mfp/integration/http" xmlns:mfp="http://www.ibm.com/mfp/integration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <displayName>nolUploadService</displayName>
    <description>nolUploadService</description>
    <connectivity>
        <connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
            <protocol>https</protocol>
            <domain>rtamhtibstgamx1.rtatestdom.local</domain>
            <port>11064</port>
            <connectionTimeoutInMilliseconds>60000</connectionTimeoutInMilliseconds>
            <socketTimeoutInMilliseconds>60000</socketTimeoutInMilliseconds>
            <maxConcurrentConnectionsPerNode>5</maxConcurrentConnectionsPerNode>
        </connectionPolicy>
    </connectivity>
    <procedure name="uploadFileInBase64" />
    <procedure name="uploadMergeFilesInBase64" />

    <property name="rta.image.nolUploadFileSizeLimit"  description="rta.image.nolUploadFileSizeLimit" defaultValue="1048576"/>
	<property name="rta.soap.language.header.xmlns"  description="rta.soap.language.header.xmlns" defaultValue="http://www.rta.ae/ActiveMatrix/ESB/AcceptLanguage/XMLSchema/"/>
	<property name="wsse.tibco.username"  description="wsse.tibco.username" defaultValue=""/>
    <property name="wsse.tibco.password"  description="wsse.tibco.password" defaultValue=""/>
	<property name="rta.pt.nolUploadService.endPoint"  description="rta.pt.nolUploadService.endPoint" defaultValue="/mobilefileuploadservice"/>
</mfp:adapter>
