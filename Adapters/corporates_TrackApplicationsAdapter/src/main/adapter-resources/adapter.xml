<mfp:adapter name="corporates_TrackApplicationsAdapter" xmlns:http="http://www.ibm.com/mfp/integration/http" xmlns:mfp="http://www.ibm.com/mfp/integration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <displayName>corporates_TrackApplicationsAdapter</displayName>
    <description>corporates_TrackApplicationsAdapter</description>
    <connectivity>
        <connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
            <protocol>https</protocol>
            <domain>${intstgrtaae}$</domain>
            <port>${intstgrtaaeport}$</port>
            <connectionTimeoutInMilliseconds>30000</connectionTimeoutInMilliseconds>
            <socketTimeoutInMilliseconds>30000</socketTimeoutInMilliseconds>
            <maxConcurrentConnectionsPerNode>2</maxConcurrentConnectionsPerNode>
        </connectionPolicy>
    </connectivity>
    <procedure name="getApplications" />
</mfp:adapter>
