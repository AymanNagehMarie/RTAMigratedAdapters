<mfp:adapter name="corporate_eTraffic_OrganizationsService" xmlns:http="http://www.ibm.com/mfp/integration/http" xmlns:mfp="http://www.ibm.com/mfp/integration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <displayName>corporate_eTraffic_OrganizationsService</displayName>
    <description>corporate_eTraffic_OrganizationsService</description>
    <connectivity>
        <connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
            <protocol>https</protocol>
            <domain>${eiptibamrtaglobal}$</domain>
            <port>${eiptibamrtaglobalport11082}$</port>
            <connectionTimeoutInMilliseconds>60000</connectionTimeoutInMilliseconds>
            <socketTimeoutInMilliseconds>30000</socketTimeoutInMilliseconds>
            <maxConcurrentConnectionsPerNode>2</maxConcurrentConnectionsPerNode>
        </connectionPolicy>
    </connectivity>
    <procedure name="getActivitiesGroupList" />
    <procedure name="getActivitiesGroupListByTrafficNo" />
    <procedure name="getActivityGroup" />
    <procedure name="getLegalClasses" />
    <procedure name="getOrganizationInfo" />


    <property name="tokens.tipcoService.username"  description="tokens.tipcoService.username" defaultValue="${tipcoServiceUsername}$"/>
    <property name="tokens.tipcoService.password"  description="tokens.tipcoService.password" defaultValue="${tipcoServicePassword}$"/>
</mfp:adapter>
